<launch>
    <arg name="cheetah_namespace" default="cheetah" />
    <param name="use_sim_time" value="true" />

    <!-- Run cheetah estimator publisher -->
    <rosparam file="$(find cheetah_inekf_lcm)/config/settings.yaml" />
    <!-- Initialize inEKF params -->
    <rosparam file="$(find cheetah_inekf_lcm)/config/inekf.yaml" />
    <!-- Initialize inEKF noise -->
    <rosparam file="$(find cheetah_inekf_lcm)/config/noise.yaml" />
    <!-- Initialize inEKF prior -->
    <rosparam file="$(find cheetah_inekf_lcm)/config/prior.yaml" />
</launch>